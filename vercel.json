{
  "version": 2,
  "buildCommand": null,
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/inmuebles",
      "destination": "/inmuebles.html"
    },
    {
      "source": "/admin",
      "destination": "/admin.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "cleanUrls": true,
  "github": {
    "enabled": true
  }
}
